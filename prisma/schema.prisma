generator client {
  provider = "prisma-client-js"
} 

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  name     String  
  email    String  @unique
  password String
  urls     URL[]
  createdAt DateTime @default(now())
}

model URL {
  id        String  @id @default(uuid())
  original  String
  short     String  @unique
  user      User?   @relation(fields: [userId], references: [id])
  userId    String?
  createdAt DateTime @default(now())
}
